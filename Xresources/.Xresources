
! Font (change size as needed)
XTerm*faceName: Iosevka
XTerm*faceSize: 23
XTerm*cursorBlink: true


! paste 
XTerm.vt100.translations: #override \
  Ctrl <Key>V: insert-selection(CLIPBOARD)

XTerm*backarrowKey: true
XTerm*backarrowIsErase: true
XTerm*ttyModes: erase ^?
XTerm*termName: xterm


! Colors
XTerm*foreground: #d6d8eb
XTerm*background: #15171c
XTerm*cursorColor: #abb2bf

XTerm*color0:  #282c34
XTerm*color1:  #e06c75
XTerm*color2:  #98c379
XTerm*color3:  #e5c07b
XTerm*color4:  #61afef
XTerm*color5:  #c678dd
XTerm*color6:  #56b6c2
